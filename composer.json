{
  "name": "weprodev/larapanel",
  "description": "LaraPanel is a user interface for managing users and permissions.",
  "type": "project",
  "homepage": "https://weprodev.com",
  "keywords": [
    "Project",
    "LaraPanel",
    "laraval",
    "User Management",
    "Spatie",
    "laravel permission",
    "Laravel User Management"
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0|^10.0|^11.0",
    "livewire/livewire": "^3.4",
    "spatie/laravel-permission": "^6.4"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "WeProDev Team",
      "email": "info@weprodev.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "WeProDev\\LaraPanel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WeProDev\\LaraPanel\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests",
    "lint": "./vendor/bin/pint"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "laravel/pint": "^1.14",
    "phpstan/phpstan": "^1.10"
  },
  "extra": {
    "laravel": {
      "providers": [
        "WeProDev\\LaraPanel\\LaraPanelProvider"
      ]
    }
  }
}
